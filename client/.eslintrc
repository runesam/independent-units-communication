{
  "extends": "airbnb",
  "rules": {
    "no-console": 0,
    "indent": 0,
    "no-tabs": 0,
    "no-mixed-spaces-and-tabs": 0,
    "no-confusing-arrow": 0,
    "no-unused-expressions": 0,
    "import/no-extraneous-dependencies": 0,
    "react/jsx-indent": 0,
    "react/jsx-filename-extension": 0
  },
  "env": {
    "jest": true,
    "es6": true,
    "node": true,
    "browser": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true,
      "experimentalObjectRestSpread": true
    }
  },
  "globals": {
    "global": true,
    "document": true
  },
  "settings": {
    "import/resolver": {
      "node": {
        "paths": [
          "client"
        ]
      }
    }
  }
}